package main;

public class Line {
	
	private Point p1;
	private Point p2;
	
	public Line (Point p1, Point p2)
	{
		
	}
	
	public int getLength()
	{
		return 0;
	}
	
	public int getSlope()
	{
		return 0;
	}
	
	public Point getPoint1()
	{
		return p1;
	}
	
	public Point getPoint2()
	{
		return p2;
	}

}
